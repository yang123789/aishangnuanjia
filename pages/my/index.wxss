.header {
  height: 245rpx;
  width:100%;
  overflow: hidden;
  background: -webkit-linear-gradient(#ffa002, #fc7926); /* Safari 5.1 - 6.0 */
  -webkit-background: -webkit-linear-gradient(#ffa002, #fc7926);
  top:0;
  padding-top:80rpx;
}

.van-cell__left-icon-wrap,.van-cell__right-icon-wrap{
  color: #FF6B29!important;
}
.tag-c-c{
  margin-left:0.58em;
  padding-top:0.28em!important;
}
.header .avatar {
  border-radius: 50%;
  overflow: hidden;
  width: 120rpx;
  height: 120rpx;
  text-align: center;
  background: #fff;
  border: 2px solid #fff;
  box-shadow: 0px 0px 15px rgba(0, 0, 0, 0.1);
  display: inline-block;
  vertical-align: middle;
  margin-right:30rpx;
  margin-left:20rpx;
  
}
.header .avatar image {
  width: 100%;
  height: 100%;
}
.header .account {
  color: #fff;
  text-align: center;
  display: inline-block;
  vertical-align: middle;
}
.showname{
  font-size: 36rpx;
}

.title-footer .cont {
  background-color:#f8f8f8;
}
.buttons_froup{
  margin-top: 1em;
}

.title-footer{
  margin-top:1em;
}
.view_notice_contain {display: flex;flex-direction: row;justify-content: center;align-items: center;height: 120rpx;background: white;font-size: 26rpx;margin:0 auto;margin-top:-60rpx;position: relative;width:706rpx;box-shadow: 0 0 20rpx rgb(194, 194, 194);border-radius:15rpx;margin-bottom: 36rpx;}
.image_notice {margin-left: 5rpx;}
.view_notice_contain .swiper_container {height: 120rpx;padding-left:30rpx;padding-right:30rpx;width:100%;padding-top:25rpx;}
.view_notice_contain .swiper_item {font-size: 26rpx;color: #666;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;letter-spacing: 2px;}
.view_notice_contain .img{width:152rpx;height:98rpx;display: inline-block;vertical-align: middle;}
.view_notice_contain .swiper_item{width:400rpx;display: inline-block;vertical-align: middle;margin-left:20rpx;
}
.all_notice{position: absolute;right:30rpx;top:40rpx;}
.all_notice image{width:20rpx;height:35rpx;display: inline-block;vertical-align: middle;}
.all_notice text{display: inline-block;vertical-align: middle;margin-right:14rpx;color: #666;}
.my-module{background:white;width:646rpx;margin:0 auto;margin-bottom:20rpx;padding:30rpx;}
.my-module .m-title{font-size: 30rpx;color: #242424;line-height: 40rpx;border-bottom: 1rpx solid #D4D4D4;padding-bottom: 20rpx;}
.my-module .m-matter{display: flex;}
.my-module .m-item{flex:1;text-align: center;padding:40rpx 0rpx 20rpx 0rpx;}
.my-module .m-item image{width:60rpx;height:60rpx;margin-bottom: 16rpx;}
.my-module .m-item view{font-size: 26rpx;color: #666666;}